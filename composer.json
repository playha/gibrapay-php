{
    "name": "gibrapay/gibrapay-php",
    "description": "Official PHP client library for the GibraPay API",
    "type": "library",
    "license": "MIT",
    "autoload": {
      "psr-4": {
        "GibraPay\\": "src/"
      }
    },
    "require": {
      "php": ">=7.3",
      "guzzlehttp/guzzle": "^7.0",
      "gibrapay/gibrapay-php": "^1.0"
